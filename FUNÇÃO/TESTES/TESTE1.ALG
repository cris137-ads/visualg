
funcao media(a:real;b:real):real
inicio
      retorne (a +b)/2
fimfuncao




Algoritmo "teste_1"

Var
   altura1,altura2,resultado:real


Inicio
      escreval("Digite duas alturas: ")
      leia (altura1)
      leia (altura2)
      
      resultado <- media (altura1,altura2)
      
      escreval (" RESULTADO=" , RESULTADO:4:2)


Fimalgoritmo